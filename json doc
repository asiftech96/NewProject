package com.atechjson.json;

import java.io.FileNotFoundException;
import java.io.FileReader;
import java.io.IOException;
import org.json.simple.JSONArray;
import org.json.simple.JSONObject;
import org.json.simple.parser.JSONParser;
import org.json.simple.parser.ParseException;
import org.junit.Test;
import org.junit.runner.RunWith;
import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.boot.test.context.SpringBootTest;
import org.springframework.boot.test.context.SpringBootTest.WebEnvironment;
import org.springframework.scheduling.annotation.Scheduled;
import org.springframework.stereotype.Service;
import org.springframework.test.context.junit4.SpringRunner;


@Service
@RunWith(SpringRunner.class)
@SpringBootTest(webEnvironment = WebEnvironment.RANDOM_PORT)
public class Jsonread {

	@Autowired
	private BillerRepository billerRepository;

	@Test
	@Scheduled(cron = "*/10 * * * * *")
	public void testBiller() {

		JSONParser jsonParser = new JSONParser();

		try (FileReader reader = new FileReader("D:\\_projects\\Asif\\HGA1F0A3990000021241.json")) {
			// Read JSON file
			Object obj = jsonParser.parse(reader);

			JSONArray billerList = (JSONArray) obj;
			System.out.println(billerList);

			// Iterate over biller array
			billerList.forEach(emp -> parseBillerObject((JSONObject) emp));

		} catch (FileNotFoundException e) {
			e.printStackTrace();
		} catch (IOException e) {
			e.printStackTrace();
		} catch (ParseException e) {
			e.printStackTrace();
		}
	}

	private void parseBillerObject(JSONObject biller) {

//		  JSONArray authenticatorsObject = (JSONArray) biller.get("authenticators");

		String objectid = (String) biller.get("objectid");
		System.out.println(objectid);

		String billerid1 = (String) biller.get("billerid1");
		System.out.println(billerid1);

		String biller_legal_name = (String) biller.get("biller_legal_name");
		System.out.println(biller_legal_name);

		String biller_name = (String) biller.get("biller_name");
		System.out.println(biller_name);

		String biller_location = (String) biller.get("biller_location");
		System.out.println(biller_location);

		String biller_category = (String) biller.get("biller_category");
		System.out.println(biller_category);

		String biller_reg_address = (String) biller.get("biller_reg_address");
		System.out.println(biller_reg_address);

		String biller_reg_city = (String) biller.get("biller_reg_city");
		System.out.println(biller_reg_city);

		String biller_reg_pin = (String) biller.get("biller_reg_pin");
		System.out.println(biller_reg_pin);

		String biller_reg_state = (String) biller.get("biller_reg_state");
		System.out.println(biller_reg_state);

		String biller_reg_country = (String) biller.get("biller_reg_country");
		System.out.println(biller_reg_country);

		String biller_mode = (String) biller.get("biller_mode");
		System.out.println(biller_mode);

		String biller_status = (String) biller.get("biller_status");
		System.out.println(biller_status);

		String biller_created_date = (String) biller.get("biller_created_date");
		System.out.println(biller_created_date);

		String biller_lastmodified_date = (String) biller.get("biller_lastmodified_date");
		System.out.println(biller_lastmodified_date);

		String biller_logo = (String) biller.get("biller_logo");
		System.out.println(biller_logo);

		String biller_bill_copy = (String) biller.get("biller_bill_copy");
		System.out.println(biller_bill_copy);

		String biller_type = (String) biller.get("biller_type");
		System.out.println(biller_type);

		String partial_pay = (String) biller.get("partial_pay");
		System.out.println(partial_pay);

		String pay_after_duedate = (String) biller.get("pay_after_duedate");
		System.out.println(pay_after_duedate);

		String online_validation = (String) biller.get("online_validation");
		System.out.println(online_validation);

		String isbillerbbps = (String) biller.get("isbillerbbps");
		System.out.println(isbillerbbps);

		String paymentamount_validation = (String) biller.get("paymentamount_validation");
		System.out.println(paymentamount_validation);

		String bbps_billerid = (String) biller.get("bbps_billerid");
		System.out.println(bbps_billerid);

		String bill_presentment = (String) biller.get("bill_presentment");
		System.out.println(bill_presentment);

		Biller biller1 = new Biller();
		biller1.setObjectid(objectid);
		biller1.setBillerid1(billerid1);
		biller1.setBiller_legal_name(biller_legal_name);
		biller1.setBiller_name(biller_name);
		biller1.setBiller_location(biller_location);
		biller1.setBiller_name(biller_category);
		biller1.setBiller_name(biller_reg_address);
		biller1.setBiller_name(biller_reg_city);
		biller1.setBiller_name(biller_reg_pin);
		biller1.setBiller_name(biller_reg_state);
		biller1.setBiller_name(biller_reg_country);
		biller1.setBiller_name(biller_mode);
		biller1.setBiller_name(biller_status);
		biller1.setBiller_name(biller_created_date);
		biller1.setBiller_name(biller_lastmodified_date);
		biller1.setBiller_name(biller_logo);
		biller1.setBiller_name(biller_bill_copy);
		biller1.setBiller_name(biller_type);
		biller1.setBiller_name(partial_pay);
		biller1.setBiller_name(pay_after_duedate);
		biller1.setBiller_name(online_validation);
		biller1.setBiller_name(isbillerbbps);
		biller1.setBiller_name(paymentamount_validation);
		biller1.setBiller_name(bbps_billerid);
		biller1.setBiller_name(bill_presentment);

		billerRepository.save(biller1);
	}

}
